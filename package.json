{
  "name": "@mondash/yarn2-pnp-testing",
  "packageManager": "yarn@3.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "tsc -b",
    "changeset": "changeset",
    "check:lint": "yarn g:lint .",
    "check:style": "yarn g:style --check .",
    "check:types": "yarn build",
    "clean": "rimraf build packages/*/dist",
    "fix:style": "yarn g:style --write .",
    "package:publish": "yarn build && changeset publish",
    "package:version": "changeset version && yarn fix:style",
    "g:lint": "eslint --config config/.eslintrc.yml --ignore-path config/.eslintignore",
    "g:style": "prettier --config config/.prettierrc.json --ignore-path config/.prettierignore"
  },
  "devDependencies": {
    "@changesets/cli": "^2.16.0",
    "@jest/types": "^27.0.6",
    "@types/eslint": "^7.28.0",
    "@types/node": "^16.4.12",
    "@types/prettier": "^2.3.2",
    "@types/rimraf": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^27.0.6",
    "jest-watch-typeahead": "^0.6.4",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
